<table id="r_table">
    <thead>
        <h1>Ranks</h1>
        <tr>
            <th>Rank ID</th>
            <th>Title</th>
        </tr>
    </thead>

    <tbody>
        {{#each r_data}}
        <tr data-value={{this.rank_id}}>
            <td>{{this.rank_id}}</td>
            <td>{{this.title}}</td>
            <td><button onclick="deleteRank({{this.rank_id}})">Delete</button></td>
        </tr>
        {{/each}}
    </tbody>
</table>

<script src="./js/delete_rank.js"></script>

<p>Add a new Rank:</p>
<form id="add-r-form-ajax">
    <label for="input-title">Title:</label>
    <input id="input-title" name = "input-title" type="text"></input>

    <input type="submit">
</form>

<script src="./js/add_rank.js"></script>
